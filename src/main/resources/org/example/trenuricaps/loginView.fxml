<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<?import java.net.URL?>
<AnchorPane prefHeight="150.0" prefWidth="214.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.trenuricaps.controller.LoginController">
   <stylesheets>
      <URL value="@styles.css" />
   </stylesheets>
   <children>
      <VBox alignment="CENTER" spacing="20" layoutX="20" layoutY="50">
         <Button fx:id="loginButton" onAction="#handleLogin" mnemonicParsing="false" prefHeight="20.0" prefWidth="200.0" text="Open New Client Window" />
      </VBox>
   </children>
</AnchorPane>